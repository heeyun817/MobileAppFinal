<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:key="etc"
        app:title="프로필 설정">
        <EditTextPreference
            app:key="id"
            app:title="닉네임 설정"/>
        <ListPreference
            app:key="gender"
            app:title="성별 선택"
            android:entries="@array/gender"
            android:entryValues="@array/my_gender_values"
             />
        <EditTextPreference
            app:key="addr"
            app:title="주소 설정"/>
        <EditTextPreference
            app:key="age"
            app:title="나이 설정"/>
    </PreferenceCategory>
</PreferenceScreen>